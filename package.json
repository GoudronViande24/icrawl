{
	"name": "icrawl",
	"version": "0.0.0",
	"description": "I write code.",
	"private": true,
	"scripts": {
		"test": "playwright test",
		"test:unit": "vitest",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"dev": "vite dev",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint src --ext .mjs,.js,.cjs,.ts,.tsx,.svelte --format=pretty",
		"format": "prettier --write . && eslint src --ext .mjs,.js,.cjs,.ts,.tsx,.svelte --fix --format=pretty"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/iCrawl/icrawl.git"
	},
	"bugs": {
		"url": "https://github.com/iCrawl/icrawl/issues"
	},
	"homepage": "",
	"devDependencies": {
		"@fontsource/inter": "^4.5.14",
		"@iconify-json/fa6-brands": "^1.1.8",
		"@iconify-json/octicon": "^1.1.26",
		"@playwright/test": "^1.29.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-vercel": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@types/cookie": "^0.5.1",
		"@unocss/cli": "^0.48.0",
		"@unocss/reset": "^0.48.0",
		"clsx": "^1.2.1",
		"eslint": "^8.31.0",
		"eslint-config-neon": "^0.1.40",
		"eslint-formatter-pretty": "^4.1.0",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"typescript": "^4.9.4",
		"unocss": "^0.48.0",
		"vercel": "^28.10.1",
		"vite": "^4.0.3",
		"vitest": "^0.26.3"
	},
	"packageManager": "yarn@3.2.4"
}
